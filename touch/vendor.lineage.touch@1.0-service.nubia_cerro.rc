on boot
    chown system system /proc/touchscreen/play_game

service vendor.touch-hal-1-0 /vendor/bin/hw/vendor.lineage.touch@1.0-service.nubia_cerro
    interface vendor.lineage.touch@1.0::IHighTouchPollingRate default
    class hal
    user system
    group system
